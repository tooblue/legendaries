---
ip: "192.168.10.11"
memory: 2048
cpus: 1
hostname: legendaries.dev
name: legendaries
provider: virtualbox

folders:
    - map: .
      to: "/home/vagrant/legendaries"

sites:
    - map: legendaries.dev
      to: "/home/vagrant/legendaries/public"

databases:
    - legendaries

variables:
    - key: APP_ENV
      value: local

